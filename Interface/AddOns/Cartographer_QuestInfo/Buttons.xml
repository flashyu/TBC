<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">

	<Button name="CQI_StartButton" parent="QuestLogDetailScrollChildFrame" hidden="true">
		<Size>
			<AbsDimension x="16" y="16"/>
		</Size>
		<Anchors>
			<Anchor point="TOPRIGHT" relativeTo="QuestLogDetailScrollChildFrame" relativePoint="TOPRIGHT">
				<Offset>
					<AbsDimension x="-35" y="-5"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture file="Interface\GossipFrame\AvailableQuestIcon">
					<Anchors>
						<Anchor point="CENTER" relativePoint="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
		<NormalTexture>
			<Size>
				<AbsDimension x="16" y="16"/>
			</Size>
			<Anchors>
				<Anchor point="CENTER" relativePoint="CENTER"/>
			</Anchors>
		</NormalTexture>
		<HighlightTexture alphaMode="ADD" file="Interface\QuestFrame\UI-QuestTitleHighlight">
			<Size>
				<AbsDimension x="16" y="16"/>
			</Size>
			<Anchors>
				<Anchor point="CENTER" relativePoint="CENTER"/>
			</Anchors>
		</HighlightTexture>
		<Scripts>
			<OnClick>
				Cartographer_QuestInfo:OnButtonClick("start", arg1);
			</OnClick>
			<OnEnter>
				Cartographer_QuestInfo:OnButtonTooltip("start");
			</OnEnter>
			<OnLeave>
				CQI_Tooltip:Hide();
			</OnLeave>
		</Scripts>
	</Button>

